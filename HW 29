import sqlite3
import os

from typing import List, Set

db_pass = os.path.join(os.getcwd(), 'chinook.db')
connection = sqlite3.connect(db_pass)
def excute_query(connection,
                 query_sql: str) ->List:
    ...
    cur = connection.cursor()
    result = cur.execute(query_sql)
    return result

def get_employees():
    query_sql = f'''
        SELECT
            FROM employees;
    '''
    result = excute_query(connection, query_sql)

def get_customers() -> Set:
    query_sql = f'''
        SELECT FirstName
            FROM customers
'''
    records = excute_query(connection, query_sql)
    names = set()
    for record in records:
        names.add(record[0])
    return names

new_list = {i: list(get_customers()).count(i) for i in get_customers()}
print(new_list)
